{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "$": false
    },
    "rules": {
        "strict": 0,
        "quotes": [1, "single"],
        "strict": 0,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/react-in-jsx-scope": 2,
        "no-loop-func": 0,
        "no-underscore-dangle": 0,
        "no-alert": 0,
        "eol-last": 0
    },
    "plugins": [
        "react"
    ]
}